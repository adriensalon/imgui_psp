prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: imgui
Description: Dear ImGui: Bloat-free Graphical User interface for C++ with minimal dependencies (docking branch)
Version: @imgui_version@
Libs: -L${libdir} -limgui
Cflags: -I${includedir} -DIMGUI_DISABLE_DEFAULT_SHELL_FUNCTIONS
